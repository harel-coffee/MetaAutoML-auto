@code {
    [Parameter]
    public GetModelResponseDto Model
    { 
        get
        {
            return _model;
        } 
        set
        {
            _model = value;
            StateHasChanged();
        } 
    }
    private GetModelResponseDto _model;
    private bool _downloadReady = true;
    private TestAutoMLResponseDto _predictionResult;

}
